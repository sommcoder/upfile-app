{
  "exclude": ["README.md, node_modules"],
  "include": ["theme-extension-src/**/*.ts", "global.d.ts"],
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "target": "ES2022",
    //    "outDir": "./extensions/theme-app-blocks/assets",
    "moduleResolution": "Bundler"
  }
}
